generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Channel {
  id          Int      @id
  name        String
  latitude    Float
  longitude   Float
  field1      String
  field2      String
  field3      String
  lastEntryId Int
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}

model Feed {
  id        Int      @id @default(autoincrement())
  entryId   Int      @unique
  createdAt DateTime
  field1    Float?
  field2    Float?
  field3    Float?
}

model ApiKey {
  id  Int    @id @default(autoincrement())
  api String @unique
}

// New User model
model User {
  id        Int    @id @default(autoincrement())
  email     String @unique
  password  String
  firstName String?
}
